<div class="row justify-content-center">
  <div class="card p-3 m-3 w-50">
    <h2>Make a new cocktail</h2>
    <%= simple_form_for @cocktail do |f| %>
      <%= f.input :name %>
      <%= f.input :photo, as: :file %>
      <%= f.submit "Create Cocktail", class: "btn btn-primary"  %>
    <% end %>
  </div>
</div>
